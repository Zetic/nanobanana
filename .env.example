# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Google GenAI API Key
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (for meme generation and voice bot)
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Realtime API Model (for voice bot feature)
# Default: gpt-4o-realtime-preview-2024-10-01
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-10-01

# XAI API Key (for Grok voice bot)
XAI_API_KEY=your_xai_api_key_here

# XAI Voice Model (for Grok voice bot)
# Default: grok-2-voice-1212
XAI_VOICE_MODEL=grok-2-voice-1212

# XAI Voice (for Grok voice bot)
# Options: ara, ash, ballad, coral, sage, verse
# Default: ara
XAI_VOICE=ara

# Elevated Users (Discord IDs) - comma-separated list
# Users in this list are not bound by usage limitations and can use special commands
ELEVATED_USERS=123456789012345678,987654321098765432

# Debug Logging (optional)
# Set to 'true' to enable detailed debug logging for voice sessions
# This provides timing information and event tracking for diagnosing slow response times
# Useful for troubleshooting voice pipeline issues
DEBUG_LOGGING=false